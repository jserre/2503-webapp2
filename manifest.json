{
  "manifest_version": 3,
  "name": "inNotion",
  "version": "1.0.0",
  "description": "A Notion-like interface for Chrome side panel",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Open inNotion",
    "default_icon": "icon128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": [
    "sidePanel",
    "storage"
  ]
}
